<p id="notice"><%= notice %></p>

<h1>Vehicles</h1>

<table>
  <thead>
    <tr>
      <th>Vin</th>
      <th>Model</th>
      <th>Make</th>
      <th>Year</th>
      <th>Times view</th>
      <th>Pic url</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @vehicles.each do |vehicle| %>
      <tr>
        <td><%= vehicle['vin'] %></td>
        <td><%= vehicle['model'] %></td>
        <td><%= vehicle['make'] %></td>
        <td><%= vehicle['year'] %></td>
        <td><%= vehicle['times_view'] %></td>
        <td><%= vehicle['price'] %></td>
        <td><%= link_to 'Show', "/vehicles/#{vehicle['vin']}?#{url_params(vehicle)}" %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Vehicle', new_vehicle_path %>
